# 서버 설정
PORT=3000                    # 서버가 실행될 포트 번호

# 데이터베이스 설정
DB_NAME=myapp               # 데이터베이스 이름
DB_USER=root                # 데이터베이스 사용자
DB_PASSWORD=your_password   # 데이터베이스 비밀번호

# API 설정
API_KEY=your_api_key       # API 키 (실제 키로 변경 필요)

# 환경 설정
NODE_ENV=development       # development 또는 production
DEBUG=false               # 디버그 모드 설정 (true/false)

# 이 파일은 예시입니다!
# 1. 이 파일을 복사해서 .env 파일을 만드세요
# 2. .env 파일에 실제 값을 입력하세요
# 3. .env 파일은 절대 깃에 커밋하지 마세요!